<%= form_for @restaurant do |f| %>
    <%= f.label :restaurant_name %>
    <%= f.text_field :restaurant_name %>
    <br>
    <br>
    <%= f.label :address %>
    <%= f.text_field :address %>
    <br>
    <br>
    <%= f.label :city %>
    <%= f.text_field :city%>
    <br>
    <br>
    <%= f.label :state %>
    <%= f.text_field :state %>
    <br>
    <br>
        <%= f.fields_for :cuisine, Cuisine.new do |c| %>
        <%= c.label :name, "Cuisine:" %>
        <%= c.text_field :name %>
    <% end %>
    <br>
    <br>
    <%= f.submit %>
    <br>
<% end %>